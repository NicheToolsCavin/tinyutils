{
  "version": 2,
  "functions": {
    "api/check.js": { "runtime": "edge" },
    "api/sitemap-delta.js": { "runtime": "edge" },
    "api/wayback-fixer.js": { "runtime": "edge" },
    "api/metafetch.js": { "runtime": "edge" }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    },
    {
      "source": "/tools/wayback-fixer/demo/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ]
}

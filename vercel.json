{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/tools", "destination": "/tools/index.html" },
    { "source": "/tools/", "destination": "/tools/index.html" },
    { "source": "/tools/sitemap-delta", "destination": "/tools/sitemap-delta/index.html" },
    { "source": "/tools/sitemap-delta/", "destination": "/tools/sitemap-delta/index.html" },
    { "source": "/tools/wayback-fixer", "destination": "/tools/wayback-fixer/index.html" },
    { "source": "/tools/wayback-fixer/", "destination": "/tools/wayback-fixer/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    },
    {
      "source": "/tools/wayback-fixer/demo/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ]
}
